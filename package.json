    "storybook:watch": "start-storybook -p 5001 --quiet -s ./public",
    "generate": "./bin/generate.js",
    "storybook:deploy": "storybook-to-ghpages",
    "lint": "eslint '*/**/*.{ts,tsx}' --quiet --fix",
    "test": "echo \"No Tests Found üßê\" ",
    "prettier": "prettier --check --write ."
  },
